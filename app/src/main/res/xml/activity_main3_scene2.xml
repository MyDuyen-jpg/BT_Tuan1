<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyPosition
               motion:motionTarget="@+id/imageView5"
               motion:framePosition="54"
               motion:keyPositionType="pathRelative"
               motion:percentX="0.5"
               motion:percentY="0.2" />
           <KeyPosition
               motion:motionTarget="@+id/imageView7"
               motion:framePosition="54"
               motion:keyPositionType="pathRelative"
               motion:percentX="0.5"
               motion:percentY="0.2" />
           <KeyPosition
               motion:motionTarget="@+id/imageView8"
               motion:framePosition="54"
               motion:keyPositionType="pathRelative"
               motion:percentX="0.5"
               motion:percentY="0.2" />
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/button" />
        <OnClick motion:targetId="@+id/button2" />
        <OnClick motion:targetId="@+id/button3" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="58dp"
            android:layout_marginTop="4dp"
            android:layout_width="95dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp"
            android:layout_marginStart="160dp"
            android:id="@+id/button" />
        <Constraint
            android:layout_height="49dp"
            android:layout_marginTop="4dp"
            android:layout_width="99dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView8"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="160dp"
            android:layout_marginStart="160dp"
            android:id="@+id/button3" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="68dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="168dp"
            android:layout_marginStart="168dp"
            android:id="@+id/imageView5" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="332dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="168dp"
            android:layout_marginStart="168dp"
            android:id="@+id/imageView8" />
        <Constraint
            android:layout_height="53dp"
            android:layout_marginTop="5dp"
            android:layout_width="98dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView7"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="168dp"
            android:layout_marginStart="168dp"
            android:id="@+id/button2" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="204dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="168dp"
            android:layout_marginStart="168dp"
            android:id="@+id/imageView7" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="58dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="95dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="168dp"
            android:layout_marginStart="168dp"
            android:id="@+id/button"
            android:layout_marginTop="120dp"
            android:alpha="1" />
        <Constraint
            android:layout_height="53dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="98dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="156dp"
            android:layout_marginStart="156dp"
            android:id="@+id/button2"
            android:layout_marginTop="244dp" />
        <Constraint
            android:layout_height="49dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="99dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="156dp"
            android:layout_marginStart="156dp"
            android:id="@+id/button3"
            android:layout_marginTop="388dp" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="540dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="180dp"
            android:layout_marginStart="180dp"
            android:id="@+id/imageView5"
            android:alpha="0" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="520dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="184dp"
            android:layout_marginStart="184dp"
            android:id="@+id/imageView7"
            android:alpha="0" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="544dp"
            motion:layout_constraintTop_toTopOf="@+id/textView"
            android:layout_width="88dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="180dp"
            android:layout_marginStart="180dp"
            android:id="@+id/imageView8"
            android:alpha="0" />
    </ConstraintSet>
</MotionScene>