<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/imageView6" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="52dp"
            motion:layout_constraintHorizontal_bias="0.498" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView3"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="52dp"
            motion:layout_constraintHorizontal_bias="0.501" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.504"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="56dp" />
        <Constraint
            android:layout_height="74dp"
            android:layout_width="81dp"
            android:id="@+id/imageView6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.512"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="56dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="179dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView2"
            android:layout_width="wrap_content"
            android:id="@+id/imageView3"
            motion:layout_constraintBottom_toTopOf="@+id/imageView6"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView3"
            motion:layout_constraintBottom_toTopOf="@+id/imageView6"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="60dp"
            android:layout_marginStart="60dp"
            android:id="@+id/imageView4"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="120dp"
            motion:layout_constraintBottom_toTopOf="@+id/imageView6"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp"
            android:id="@+id/imageView2" />
        <Constraint
            android:layout_height="78dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="56dp"
            android:layout_width="82dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="164dp"
            android:layout_marginStart="164dp"
            android:id="@+id/imageView6" />
    </ConstraintSet>
</MotionScene>